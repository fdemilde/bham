{
  "name": "bhamdesigns",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-deps": "rimraf node_modules",
    "html": "pug src/*.pug --watch -o public",
    "css": "postcss -u autoprefixer -r public/css/*.css",
    "sass": "node-sass --output-style compressed -o public/css/ src/sass/styles.scss  && npm run css",
    "sass:w": "nodemon -e scss -x \"npm run sass\"",
    "js": "babel src/js --watch --out-dir public/js",
    "serve": "./node_modules/.bin/live-server --open=public --watch=PATH --host=localhost --port=8080",
    "start": "concurrently \"npm run html\" \"npm run sass:w\" \"npm run serve\""
  },
  "author": "Jon Leibham",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
    "babel-register": "^6.16.3",
    "concurrently": "^3.1.0",
    "font-awesome": "^4.6.3",
    "live-server": "^1.1.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "postcss-cli": "^2.6.0",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "tape": "^4.6.2"
  }
}
