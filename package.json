{
  "name": "bhamdesigns",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "html": "pug src/*.pug --watch -o public",
    "css": "postcss -u autoprefixer -r public/css/*.css",
    "sass": "node-sass --output-style compressed -o public/css/ src/sass/styles.scss  && npm run css",
    "sass:w": "nodemon -e scss -x \"npm run sass\"",
    "js": "babel src/js --watch --out-dir public/js",
    "serve": "./node_modules/.bin/live-server --open=public --watch=PATH --host=localhost --port=8080",
    "start": "concurrently \"npm run html\" \"npm run sass:w\" \"npm run serve\""
  },
  "author": "Jon Leibham",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.14.0",
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.16.0",
    "babel-register": "^6.14.0",
    "concurrently": "^2.2.0",
    "font-awesome": "^4.6.3",
    "live-server": "^1.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "normalize.css": "^4.2.0",
    "postcss-cli": "^2.5.2",
    "pug-cli": "^1.0.0-alpha6",
    "tape": "^4.6.2"
  }
}
